      "source": [
        "import tensorflow as tf\n",
        "from tensorflow import keras\n",
        "import matplotlib.pyplot as plt\n",
        "import numpy as np"
      ]
    },
    {
      "cell_type": "markdown",
      "id": "634c941e",
      "metadata": {
        "id": "634c941e"
      },
      "source": [
        "# Step 2: Load MNIST Dataset"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 3,
      "id": "50ccc07f",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
